%{
#include <stdio.h>
%}

%%
-?[0]+            { printf("Zero\n"); }
-[1-9][0-9]*      { printf("Negative number\n"); }
[+]?[1-9][0-9]*   { printf("Positive number\n"); }
\n                { /* ignore newline */ }
%%

int main()
{
    printf("Enter a number: ");
    yylex();
    return 0;
}

int yywrap()
{
    return 1;
}
